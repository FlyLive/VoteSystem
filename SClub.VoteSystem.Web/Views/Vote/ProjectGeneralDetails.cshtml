@model SClub.VoteSystem.LogicDemo.DataBase.VoteProject

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@Model.Name</title>
    <link href="~/Css/Project/PaojectGeneral.css" rel="stylesheet" />
</head>
<body>
    <div class="all"id="top">
        <div class="secondBox">
            <h1>《@Model.Name》</h1>
            <div class="projectImg">
                 <img class="image" src="@Model.Image_F" />
            </div>
            <div class="projectMessage">
                <h2>项目简介:</h2>
                <p>@Model.Message</p>
            </div>
            <div class="projectIntro">
                <h2>项目详情:</h2>
                <p>@Model.Intro</p>
            </div>
            <div class="projectTime">
                <h2>活动开放时间:</h2>
                <p>开始时间:@(Model.VoteActivity.BeginTime) 截至日期:@(Model.VoteActivity.EndTime)</p>
            </div>
            <div class="movie">
                <h2>视屏简介:</h2>
                <div>
                    <embed class="video" src="@Model.Movie" flashvars="vid=p001452v7j0&amp;">
                </div>
            </div>
            <div class="projectCount">
                <h2>当前票数:</h2>
                <p>@Model.VoteCount</p>
            </div>
            <div class="evaluate">
                <h2>评论:</h2>
                <div>
                    <ul>
                        @foreach (var item in Model.VoteActivity.VoteProject)
                        {
                            <li>
                                <h3>@item.Name:</h3>
                                <p>@item.Intro</p>
                            </li>
                        }
                    </ul>
                </div>
            </div>
            <div class="foot">
                <p class="foot-1">
                    <a href="#top">返回顶部</a>
                    <span>|</span>
                    <a>关于我们</a>
                    <span>|</span>
                    <a href="/UserServise/HomePage">注册</a>
                    <span>|</span>
                </p>
                <p>
                    版权所有 2016-2016 Paranoid. 重庆理工大学奇点俱乐部17-1-2-C
                </p>
                <p>
                    渝ISO备9999999号
                </p>
            </div>
        </div>
    </div>
</body>
</html>